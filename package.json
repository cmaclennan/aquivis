{
  "name": "aquivis",
  "version": "1.0.0",
  "private": true,
  "description": "Professional pool service management platform with QLD Health compliance",
  "author": "Craig",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "verify:e2e:user": "tsx scripts/verify-e2e-user.ts",
    "test:sentry:tunnel": "node scripts/test-sentry-tunnel.js",
    "prepush:verify": "node scripts/prepush-check.js",
    "start:perf": "set E2E_TEST_MODE=1 && next start",
    "perf:prep": "tsx scripts/perf-prep.ts",
    "perf:lhci": "lhci autorun",
    "perf:lh:cli": "npm run build && start-server-and-test \"set E2E_TEST_MODE=1 && set PORT=3010 && next start\" http://localhost:3010 \"tsx scripts/lighthouse-collect.ts\"",
    "perf:lh:summary": "tsx scripts/lh-summary.ts",
    "perf:api": "npm run build && start-server-and-test start:perf http://localhost:3000 \"artillery run artillery.config.json\"",
    "perf:probe": "npm run build && start-server-and-test start:perf http://localhost:3000 \"tsx scripts/perf-probe.ts\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.15",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.45.0",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.462.0",
    "next": "^15.5.4",
    "next-auth": "^5.0.0-beta.29",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.61.1",
    "recharts": "^2.15.4",
    "resend": "^3.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.56.1",
    "@sentry/nextjs": "^10.20.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.16.5",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitest/coverage-v8": "^4.0.1",
    "artillery": "^2.0.26",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-config-next": "15.1.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "msw": "^2.11.6",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "start-server-and-test": "^2.1.2",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.6",
    "typescript": "^5.8.0",
    "vitest": "^4.0.1"
  }
}
